# Example environment variables for LightRAG API
# Copy to .env and fill in values: cp .env.example .env

# REQUIRED
OPENAI_API_KEY=sk-xxxx

# OPTIONAL MODEL SETTINGS
OPENAI_MODEL=gpt-5-mini
OPENAI_TEMPERATURE=0.0
OPENAI_FALLBACK_MODELS=gpt-5-mini,gpt-4.1-mini  # Список через запятую. Пример: gpt-5-mini,gpt-4.1-mini

# RAG SETTINGS
RAG_WORKING_DIR=./documents
RAG_EMBEDDING_MODEL=gpt-5-mini
RAG_LLM_MODEL=gpt-5-mini
RAG_RERANK_ENABLED=true

# APP SETTINGS
APP_LOG_LEVEL=INFO
APP_DEBUG=false

# STARTUP BEHAVIOR
ALLOW_START_WITHOUT_OPENAI_KEY=0
FAST_FAILLESS_INIT=0

# To override .env filename you can provide ENV_FILE env var when running container
